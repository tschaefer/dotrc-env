[user]
	name = Tobias Schaefer
	email = Tobias.Schaefer@blackox.org

[color]
	ui = true

[alias]
	graph = !git log --graph --date-order -C -M --pretty=format:'%C(yellow)%h%Creset %ad %Cgreen%d%Creset %s' --all --date=short
	rstat = !git fetch && git diff --name-only ..origin
	dlog = !git log --stat --abbrev-commit

